{
    "name": "mundoeletronico",
    "version": "0.1.0",
    "description": "Projeto pessoal feito em NextJS",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/nonam4/mundoeletronico"
    },
    "author": "Luiz Carlos Ferreira",
    "bugs": {
        "url": "https://github.com/nonam4/mundoeletronico/issues"
    },
    "main": "electron.js",
    "scripts": {
        "start": "electron .",
        "windows": "electron-builder --ia32 --dir",
        "linux": "electron-builder --x64 --dir"
    },
    "devDependencies": {
        "electron": "^15.3.0",
        "electron-builder": "^22.13.1",
        "electron-reload": "^1.5.0"
    },
    "dependencies": {
        "axios": "^0.21.4",
        "electron-download-manager": "^2.1.2",
        "electron-is-dev": "^2.0.0",
        "net-snmp": "^2.10.1"
    },
    "build": {
        "appId": "ioiprinters",
        "linux": {
            "icon": "resources/icon.png",
            "target": [
                "deb"
            ]
        },
        "win": {
            "target": "NSIS",
            "icon": "icon.ico"
        }
    }
}