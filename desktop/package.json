{
    "name": "mundoeletronico",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/nonam4/mundoeletronico"
    },
    "author": "Luiz Carlos Ferreira",
    "bugs": {
        "url": "https://github.com/nonam4/mundoeletronico/issues"
    },
    "main": "public/main.js",
    "homepage": "./",
    "scripts": {
        "next:dev": "next dev",
        "start": "concurrently -k \"cross-env BROWSER=none npm run next:dev\" \"npm run electron\"",
        "build": "",
        "electron": "wait-on tcp:3000 && electron .",
        "windows": "electron-builder --ia32 --dir",
        "linux": "electron-builder --x64 --dir"
    },
    "dependencies": {
        "@electron/remote": "^2.0.4",
        "axios": "^0.25.0",
        "electron": "^16.0.8",
        "electron-builder": "^22.14.5",
        "electron-download-manager": "^2.1.2",
        "net-snmp": "^3.5.8",
        "next": "^12.0.9",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-notifications-component": "^3.1.0",
        "styled-components": "^5.3.3"
    },
    "devDependencies": {
        "concurrently": "^7.0.0",
        "cross-env": "^7.0.3",
        "electron-is-dev": "^2.0.0",
        "eslint": "7.32.0",
        "eslint-config-next": "12.0.9",
        "wait-on": "^6.0.0"
    },
    "build": {
        "appId": "ioiprinters",
        "linux": {
            "icon": "public/icon.png",
            "target": [
                "deb"
            ]
        },
        "win": {
            "target": "NSIS",
            "icon": "icon.ico"
        }
    }
}