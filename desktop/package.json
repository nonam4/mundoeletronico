{
    "name": "mundoeletronico",
    "productName": "Printers",
    "description": "Aplicativo de coleta de dados (modelo, serial, IP, contadores) de impressoras locadas",
    "version": "0.1.0",
    "private": true,
    "repository": "https://github.com/nonam4/mundoeletronico",
    "bugs": "https://github.com/nonam4/mundoeletronico/issues",
    "author": "Mundo EletrÃ´nico",
    "main": "public/electron.js",
    "homepage": "./",
    "scripts": {
        "react:start": "react-scripts start",
        "react:build": "react-scripts build",
        "react:test": "react-scripts test",
        "react:eject": "react-scripts eject",
        "electron:start": "wait-on tcp:3000 && electron .",
        "start": "concurrently -k \"cross-env BROWSER=none npm run react:start\" \"npm run electron:start\"",
        "windows": "npm run react:build && electron-builder --ia32 --dir -c.extraMetadata.main=build/electron.js",
        "linux": "electron-builder --x64 --dir"
    },
    "dependencies": {
        "@electron/remote": "^1.0.2",
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "axios": "^0.25.0",
        "electron-download-manager": "^2.1.2",
        "electron-is-dev": "^2.0.0",
        "net-snmp": "^3.5.8",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-notifications-component": "^3.3.11",
        "react-scripts": "^5.0.0",
        "styled-components": "^5.3.3",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "concurrently": "^7.0.0",
        "cross-env": "^7.0.3",
        "electron": "^11.2.1",
        "electron-builder": "^22.9.1",
        "wait-on": "^5.2.1"
    },
    "build": {
        "extends": null,
        "appId": "com.mundoeletronico",
        "files": [
            "dist/**/*",
            "build/**/*",
            "node_modules/**/*",
            "package.json"
        ],
        "directories": {
            "buildResources": "assets"
        },
        "linux": {
            "icon": "build/icon.png",
            "target": [
                "deb"
            ]
        },
        "win": {
            "target": "NSIS",
            "icon": "build/icon.ico"
        }
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}